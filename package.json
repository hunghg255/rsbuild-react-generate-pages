{
  "name": "rsbuild-react-generate-pages",
  "version": "0.0.1",
  "packageManager": "pnpm@9.11.0",
  "description": "A Vite plugin to generate pages for React",
  "type": "module",
  "keywords": [
    "reactjs",
    "vite",
    "plugin"
  ],
  "license": "MIT",
  "homepage": "https://github.com/hunghg255/react-generate-pages#readme",
  "bugs": {
    "url": "https://github.com/hunghg255/react-generate-pages/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hunghg255/react-generate-pages.git"
  },
  "files": [
    "dist",
    "client.d.ts"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./client": {
      "import": "./client.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "lint": "eslint src --quiet",
    "lint:fix": "eslint src --fix",
    "build": "rslib build",
    "dev": "rslib build --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp",
    "prepublishOnly": "pnpm run build",
    "playground": "pnpm --dir ./playground run dev"
  },
  "dependencies": {
    "@nozomuikuta/h3-cors": "^0.2.2",
    "@rsbuild/shared": "^0.0.17",
    "get-port-please": "^3.1.2",
    "h3": "^1.15.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "fs-extra": "^11.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@rsbuild/core": "^1.3.9",
    "@rslib/core": "^0.6.1",
    "@rspack/core": "^1.3.5",
    "@types/node": "^20.17.30",
    "bumpp": "^9.11.1",
    "eslint": "^8.57.1",
    "eslint-config-relaxed-ts": "^2.0.34",
    "esno": "^0.17.0",
    "magic-string": "^0.30.17",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vitest": "^0.34.6",
    "@types/fs-extra": "^11.0.4"
  }
}
